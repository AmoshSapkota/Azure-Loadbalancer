# Copy this file to terraform.tfvars and modify the values as needed

# Azure Resource Group name
resource_group_name = "your-loadbalancer-rg"

# Azure region
location = "East US"

# Virtual Network name
vnet_name = "your-vnet-lb"

# Your SSH public key (paste your public key here)
ssh_public_key = "ssh-rsa AAAAB3NzaC1yc2EAAAA... your-email@example.com"